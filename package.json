    "http-server": "^0.11.1",
    "mocha": "^5.0.5",
    "mocha-typescript": "^1.1.12",
    "nyc": "^12.0.2",
    "sinon": "^6.0.0",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.0.1",
